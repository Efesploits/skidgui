			-- Efenin headless sıfaül eşigal

			-- Instances: blah blah

			local Headlessgui = Instance.new("ScreenGui")
			local main = Instance.new("Frame")
			local Title = Instance.new("TextLabel")
			local headless = Instance.new("TextButton")
			local cre = Instance.new("TextLabel")
			local cre2 = Instance.new("TextLabel")

			--Properties: blah blah

			Headlessgui.Name = "Headless gui"
			Headlessgui.Parent = game.CoreGui
			Headlessgui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

			main.Name = "main"
			main.Parent = Headlessgui
			main.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
			main.Position = UDim2.new(0.268199205, 0, 0.287145257, 0)
			main.Size = UDim2.new(0, 338, 0, 234)
			main.Active = true
			main.Draggable = true

			Title.Name = "Title"
			Title.Parent = main
			Title.BackgroundColor3 = Color3.fromRGB(53, 53, 53)
			Title.BorderColor3 = Color3.fromRGB(53, 53, 53)
			Title.Size = UDim2.new(0, 338, 0, 53)
			Title.Font = Enum.Font.Oswald
			Title.Text = "Headless Head R15 and R6"
			Title.TextColor3 = Color3.fromRGB(0, 0, 0)
			Title.TextSize = 43.000

			headless.Name = "headless"
			headless.Parent = main
			headless.BackgroundColor3 = Color3.fromRGB(65, 65, 65)
			headless.Position = UDim2.new(0.204142004, 0, 0.43589747, 0)
			headless.Size = UDim2.new(0, 200, 0, 50)
			headless.Font = Enum.Font.GothamBlack
			headless.Text = "Headless"
			headless.TextColor3 = Color3.fromRGB(0, 0, 0)
			headless.TextSize = 39.000
			headless.MouseButton1Down:connect(function()
				local lp = game:GetService "Players".LocalPlayer
				if lp.Character:FindFirstChild "Head" then
					local char = lp.Character
					char.Archivable = true
					local new = char:Clone()
					new.Parent = workspace
					lp.Character = new
					wait(2)
					local oldhum = char:FindFirstChildWhichIsA "Humanoid"
					local newhum = oldhum:Clone()
					newhum.Parent = char
					newhum.RequiresNeck = false
					oldhum.Parent = nil
					wait(2)
					lp.Character = char
					new:Destroy()
					wait(1)
					newhum:GetPropertyChangedSignal("Health"):Connect(
					function()
						if newhum.Health <= 0 then
							oldhum.Parent = lp.Character
							wait(1)
							oldhum:Destroy()
						end
					end)
					workspace.CurrentCamera.CameraSubject = char
					if char:FindFirstChild "Animate" then
						char.Animate.Disabled = true
						wait(.1)
						char.Animate.Disabled = false
					end
					lp.Character:FindFirstChild "Head":Destroy()
				end

			end)

			cre.Name = "cre"
			cre.Parent = main
			cre.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			cre.BackgroundTransparency = 1.000
			cre.Position = UDim2.new(0, 0, 0.888888896, 0)
			cre.Size = UDim2.new(0, 191, 0, 26)
			cre.Font = Enum.Font.SourceSans
			cre.Text = "Gui made by : Grimlxcks#5721"
			cre.TextColor3 = Color3.fromRGB(0, 0, 0)
			cre.TextSize = 16.000

			cre2.Name = "cre2"
